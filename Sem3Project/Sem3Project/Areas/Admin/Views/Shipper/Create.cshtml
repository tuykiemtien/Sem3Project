@using Sem3Project.Entites
@{
    ViewBag.Title = "Create";
    var list = ViewBag.listAdmin;
}
<section class="modal-header">
    <h2>Create a Shipper</h2>
</section>
<section class="modal-body row">
    <section>
        <div><p class="text-danger" id="create-valid"></p></div>
        <form class="form-horizontal" id="form" method="post" enctype="multipart/form-data">
            <section class="col-md-6">
                <div class="form-group">
                    <label for="name" class="control-label col-sm-2">Company Name</label>
                    <div class="col-sm-10">
                        <div class="input-group">
                            <input type="text" class="form-control" id="company-name" maxlength="10" placeholder="Enter Company name" style="width:280px;" />
                            <span class="text-danger" id="company-name-valid"></span>
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <label for="name" class="control-label col-sm-2">Phone</label>
                    <div class="col-sm-10">
                        <div class="input-group">
                            <input type="text" class="form-control" id="phone" maxlength="20" placeholder="Enter phone" style="width:280px;" />
                            <span class="text-danger" id="phone-valid"></span>
                        </div>
                    </div>
                </div>
            </section>
        </form>
    </section>
</section>
<section class="modal-footer">
    <div class="form-group ">
        <div class="col-md-12">
            <input type="button" id="create" class="btn btn-primary  login-button" value="Create">
            <input type="button" id="cancel" class="btn  login-button" data-dismiss="modal" value="Cancel">
        </div>
    </div>
</section>
<script src="https://cdnjs.cloudflare.com/ajax/libs/caret/1.0.0/jquery.caret.min.js"></script>
<script src="~/Scripts/admin/Shipper/Create.js"></script>
